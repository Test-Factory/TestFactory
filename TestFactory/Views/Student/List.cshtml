@model IList<TestFactory.MVC.ViewModels.StudentViewModel>
 
    <h2>List Students</h2>
    <p>
        @Html.ActionLink("Create New", "Create", new { groupId = Request.Params["groupId"] })
    </p>
<table class="table">
    @foreach (var item in Model) { 
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Update", new { id = item.Id }) 
        </td>
    </tr>
    }
</table>


